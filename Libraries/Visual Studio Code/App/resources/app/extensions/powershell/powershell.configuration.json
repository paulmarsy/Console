				{ tokenType:'string', openTrigger: '"', open: /"$/, closeComplete: '"' },
				{ tokenType: 'string', openTrigger: '\'', open: /'$/, closeComplete: '\'' }
	],

	autoClosingPairs: [['{', '}'], ['[', ']'], ['(', ')']],	// Defined explicitly, to suppress the
