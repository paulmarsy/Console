{
	"name": "tsql",
	"version": "0.1.0",
	"publisher": "vscode",
	"engines": { "vscode": "*" },
	"activationEvents": ["onLanguage:sql"],
	"main": "./out/sqlMain",
	"contributes": {
		"languages": [{
			"id": "sql",
			"extensions": [ ".sql" ],
			"aliases": [ "SQL" ]
		}]
	},
	"isAMD": true,
	"scripts": {
		"vscode:prepublish": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../gulpfile.plugins.js compile-plugin:tsql"
	},
	"_bundling": [
		{ "name": "./out/sqlMain" }
	]
}