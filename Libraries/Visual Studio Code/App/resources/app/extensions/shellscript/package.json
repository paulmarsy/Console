{
	"name": "shellscript",
	"version": "0.1.0",
	"publisher": "vscode",
	"engines": { "vscode": "*" },
	"activationEvents": ["onLanguage:shellscript"],
	"main": "./out/shellscriptMain",
	"contributes": {
		"languages": [{
			"id": "shellscript",
			"aliases": ["Shell Script (Bash)", "shellscript"],
			"extensions": [".sh", ".bash", ".zsh", ".bashrc", ".bash_profile", ".bash_login", ".profile", ".bash_logout"],
			"firstLine": "^#!.*\\b(bash|zsh|sh|tcsh)|^#\\s*-\\*-[^*]*mode:\\s*shell-script[^*]*-\\*-"
		}],
		"grammars": [{
			"language": "shellscript",
			"scopeName": "source.shell",
			"path": "./syntaxes/Shell-Unix-Bash.tmLanguage"
		}]
	},
	"isAMD": true,
	"scripts": {
		"vscode:prepublish": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../gulpfile.plugins.js compile-plugin:shellscript"
	},
	"_bundling": [
		{ "name": "./out/shellscriptMain" }
	]
}