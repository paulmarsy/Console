{
	"name": "docker",
	"version": "0.1.0",
	"publisher": "vscode",
	"engines": { "vscode": "*" },
	"activationEvents": ["onLanguage:dockerfile"],
	"main": "./out/dockerfileMain",
	"contributes": {
		"languages": [{
			"id": "dockerfile",
			"extensions": [ ".dockerfile" ],
			"filenames": [ "Dockerfile" ],
