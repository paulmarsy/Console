{
	"name": "cpp",
	"version": "0.1.0",
	"publisher": "vscode",
	"engines": { "vscode": "*" },
	"activationEvents": ["onLanguage:cpp"],
	"main": "./out/cppMain",
	"contributes": {
		"languages": [{
			"id": "cpp",
			"extensions": [ ".cpp", ".c", ".cc", ".cxx", ".h", ".hpp", ".hh", ".hxx" ],
			"aliases": [ "C++", "Cpp", "cpp", "c" ]
		}]
	},
	"isAMD": true,
	"scripts": {
		"vscode:prepublish": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../gulpfile.plugins.js compile-plugin:cpp"
	},
	"_bundling": [
		{ "name": "./out/cppMain" }
	]
}