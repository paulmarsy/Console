{
	"name": "jade",
	"version": "0.1.0",
	"publisher": "vscode",
	"engines": { "vscode": "*" },
	"activationEvents": ["onLanguage:jade"],
	"main": "./out/jadeMain",
	"contributes": {
		"languages": [{
			"id": "jade",
			"extensions": [ ".jade" ],
			"aliases": [ "Jade", "jade" ]
		}]
	},
	"isAMD": true,
	"scripts": {
		"vscode:prepublish": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../gulpfile.plugins.js compile-plugin:jade"
	},
	"_bundling": [
		{ "name": "./out/jadeMain" }
	]
}